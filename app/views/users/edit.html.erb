
<% if false %>

<form action="/users" method="post"  accept-charset="UTF-8">
  <label for="username">Username:</label>
  <input type="text" id="username" name="user[username]"><br><br>
  <label for="email">Email:</label>
  <input type="email" id="email" name="user[email]"><br><br>
  <label for="password">Password:</label>
  <input type="password" id="password" name="user[password]"><br><br>
  <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
  <input type="submit" value="Submit">
</form>

<% end %>


<% if false %>

<%= form_tag('/users') do -%>
<%= label_tag 'username', 'Username:' %>
<%= text_field_tag 'username' %>
<br/>
<br/>
<%= label_tag 'password', 'Password:' %>
<%= password_field_tag 'password' %>
<br/>
<br/>
<%= label_tag 'email', 'Email:' %>
<%= email_field_tag 'email' %>
<br/>
<br/>
<%= submit_tag 'Submit' %>
<% end - %>

<% end %>


<%= form_for @user do |f| %>
  Your user name here: <%= f.text_field :username %><br /><br />
  Email: <%= f.email_field :email, placeholder: 'example@example.com' %><br /><br />
  Password: <%= f.password_field :password %><br /><br />
  <%= f.submit %>
<% end %>




